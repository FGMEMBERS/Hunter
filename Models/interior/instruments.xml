<?xml version="1.0"?>
<!-- Hunter model config file -->
<!-- Vivian Meazza 2004/1/2 vivian.meazza@lineone.net -->
<!-- Ver 0.1.0 -->
<PropertyList>

<animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>20</max-m>
    </animation>

	
	<!-- ***************** Cockpit *************************** -->
	<!-- Front hotspots -->
	<panel>
		<path>Aircraft/Hunter/Models/hunter-panel-hotspots.xml</path>
		<bottom-left>
			<x-m>-4.83</x-m>
			<y-m>-0.50</y-m>
			<z-m>-0.30</z-m>
		</bottom-left>
		<bottom-right>
			<x-m>-4.83</x-m>
			<y-m> 0.50</y-m>
			<z-m>-0.30</z-m>
		</bottom-right>
		<top-left>
			<x-m>-4.83</x-m>
			<y-m>-0.50</y-m>
			<z-m> 0.62</z-m>
		</top-left>
	</panel>


	<!-- **************** Instruments ********************* -->
