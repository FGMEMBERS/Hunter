	<model>
		<path>Aircraft/Hunter/Models/interior/pilot-g.xml</path>
		<offsets>
			<x-m>-4.75</x-m>
			<y-m>0.11</y-m>
			<z-m>0.625</z-m>
		</offsets>
	</model>

	<!-- ************* Right Lower Side Panel ******* -->

