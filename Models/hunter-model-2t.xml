  <object-name>Intake-L</object-name>
	<object-name>Markings</object-name>
  <object-name>Canopy</object-name>
  <object-name>Exhausts</object-name>
  <condition>
    <greater-than-equals>
      <property>/sim/current-view/view-number</property>
